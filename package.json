{
  "name": "@dimo-network/transactions",
  "version": "0.2.1",
  "description": "DIMO Transactions SDK",
  "keywords": [
    "DIMO",
    "transactions-sdk",
    "dimo-network"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "prebuild": "rm -rf ./dist && node -p \"'export const LIB_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/version.ts & npm i --package-lock-only",
    "build": "tsc -p ./tsconfig.build.json && tsc-alias",
    "dev": "tsc --watch & nodemon --watch dist --delay 1 --exec tsc-alias",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@turnkey/api-key-stamper": "^0.4.5",
    "@turnkey/http": "^2.22.0",
    "@turnkey/viem": "^0.4.31",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.25.2",
    "@wormhole-foundation/sdk": "3.4.1",
    "@wormhole-foundation/sdk-definitions-ntt": "2.0.0",
    "@wormhole-foundation/sdk-evm-ntt": "2.0.0",
    "@wormhole-foundation/sdk-route-ntt": "2.0.0",
    "@wormhole-foundation/sdk-solana-ntt": "2.0.0",
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/sdk": "^5.4.35",
    "ethers": "^6.14.0",
    "ethers-v5": "npm:ethers@^5.7.2",
    "uuid": "^11.1.0",
    "viem": "^2.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@peculiar/webcrypto": "^1.5.0",
    "@testing-library/preact": "^2.0.1",
    "@types/chai": "^5.2.2",
    "@types/node": "^14.18.63",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-preact": "^1.5.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "@wormhole-foundation/sdk": "3.4.1",
    "@wormhole-foundation/sdk-base": "3.4.1",
    "@wormhole-foundation/sdk-definitions": "3.4.1"
  },
  "author": "Allyson English",
  "license": "Apache-2.0"
}
