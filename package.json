{
  "name": "@dimo-network/transactions",
  "version": "0.2.3",
  "description": "DIMO Transactions SDK",
  "keywords": [
    "DIMO",
    "transactions-sdk",
    "dimo-network"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "prebuild": "rm -rf ./dist && node -p \"'export const LIB_VERSION = \\'' + require('./package.json').version + '\\';'\" > src/version.ts & npm i --package-lock-only",
    "build": "tsc -p ./tsconfig.build.json && tsc-alias",
    "dev": "tsc --watch & nodemon --watch dist --delay 1 --exec tsc-alias",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@turnkey/api-key-stamper": "^0.5.0",
    "@turnkey/http": "^3.12.1",
    "@turnkey/viem": "^0.14.1",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "@uniswap/v3-sdk": "^3.25.2",
    "@wormhole-foundation/sdk": "3.6.0",
    "@wormhole-foundation/sdk-definitions-ntt": "3.0.0",
    "@wormhole-foundation/sdk-evm-ntt": "3.0.0",
    "@wormhole-foundation/sdk-route-ntt": "3.0.0",
    "@wormhole-foundation/sdk-solana-ntt": "3.0.0",
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/sdk": "^5.5.2",
    "uuid": "^13.0.0",
    "viem": "^2.36.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@peculiar/webcrypto": "^1.5.0",
    "@testing-library/preact": "^3.2.4",
    "@types/chai": "^5.2.2",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-preact": "^2.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "@wormhole-foundation/sdk": "3.6.0",
    "@wormhole-foundation/sdk-base": "3.4.1",
    "@wormhole-foundation/sdk-definitions": "3.4.1"
  },
  "author": "Allyson English",
  "license": "Apache-2.0"
}
